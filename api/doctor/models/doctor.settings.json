{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "doctors",
  "info": {
    "name": "Doctores"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Nombre": {
      "type": "string"
    },
    "cedula": {
      "type": "string"
    },
    "especialidad": {
      "type": "string"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "doctor"
    },
    "horarios": {
      "collection": "horarios",
      "via": "doctor"
    },
    "consultorios": {
      "collection": "consultorio",
      "via": "doctor"
    },
    "pagos": {
      "collection": "pago",
      "via": "doctor"
    },
    "citas": {
      "collection": "cita",
      "via": "doctor"
    }
  }
}
