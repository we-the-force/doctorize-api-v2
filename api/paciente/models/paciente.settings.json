    "HIstoria_familiar": {
      "type": "component",
      "repeatable": true,
      "component": "perfil-paciente.historia-familiar"
    },
    "contraindicaciones": {
      "type": "component",
      "repeatable": true,
      "component": "perfil-paciente.contraindicaciones"
    },
    "hospitalizacion": {
      "type": "component",
      "repeatable": true,
      "component": "perfil-paciente.hospitalizaciones"
    },
    "cirugias": {
      "type": "component",
      "repeatable": true,
      "component": "perfil-paciente.cirugias"
    },
    "vacunas": {
      "type": "component",
      "repeatable": true,
      "component": "perfil-paciente.vacunas"
    },
    "medicamentos": {
      "collection": "medicamentos",
      "via": "paciente"
    },
    "vitales": {
      "collection": "vitales",
      "via": "paciente"
    },
    "consultas": {
      "collection": "consulta",
      "via": "paciente"
