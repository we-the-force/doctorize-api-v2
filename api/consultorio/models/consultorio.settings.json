{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "consultorios",
  "info": {
    "name": "Consultorios"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "correo": {
      "type": "email"
    },
    "nombre": {
      "type": "string"
    },
    "telefono": {
      "type": "biginteger"
    },
    "hospital": {
      "type": "string"
    },
    "numero": {
      "type": "integer"
    },
    "direccion": {
      "type": "text"
    },
    "lat": {
      "type": "float"
    },
    "lng": {
      "type": "float"
    },
    "doctor": {
      "model": "doctor",
      "via": "consultorios"
    },
    "HorarioApertura": {
      "type": "time"
    },
    "HorarioCierre": {
      "type": "time"
    },
    "InicioComida": {
      "type": "time"
    },
    "FinComida": {
      "type": "time"
    },
    "Dias": {
      "type": "component",
      "repeatable": true,
      "component": "consultorio.dias"
    },
    "Activo": {
      "type": "boolean",
      "default": true
    },
    "asistente_consultorios": {
      "via": "consultorio",
      "collection": "asistente-permisos"
    }
  }
}
