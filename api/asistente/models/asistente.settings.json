{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "asistentes",
  "info": {
    "name": "Asistentes"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "nombre": {
      "type": "string"
    },
    "correo": {
      "type": "email"
    },
    "img_data": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "telefono": {
      "type": "biginteger"
    },
    "fecha_nacimiento": {
      "type": "date"
    },
    "asistente_permisos": {
      "collection": "asistente-permisos",
      "via": "asistente"
    },
    "doctor": {
      "via": "asistentes",
      "model": "doctor"
    },
    "Activo": {
      "type": "boolean",
      "default": true
    },
    "horarios": {
      "via": "asistente",
      "collection": "horarios"
    }
  }
}
