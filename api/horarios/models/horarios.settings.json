      "model": "consultorio"
    },
    "doctor": {
      "via": "horarios",
      "model": "doctor"
    },
    "asistente": {
      "via": "horarios",
      "model": "asistente"
    },
    "Activo": {
      "type": "boolean",
      "default": true
    }
  }
}
